{
  "name": "antd-img-crop",
  "version": "4.12.2",
  "description": "An image cropper for Ant Design Upload",
  "keywords": [
    "react",
    "antd",
    "ant-design",
    "upload",
    "image",
    "crop",
    "cropper"
  ],
  "sideEffects": [
    "*.css"
  ],
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Ranhe <wenguang.fe@gmail.com> (https://circlereader.com/)",
  "scripts": {
    "build": "npx tsc",
